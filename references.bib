@article{QuarkCondensate,
title = {Quark pair condensation and chiral symmetry breaking in {QCD}},
journal = {Nuclear Physics B},
volume = {199},
number = {1},
pages = {168-188},
year = {1982},
issn = {0550-3213},
doi = {https://doi.org/10.1016/0550-3213(82)90570-3},
url = {https://www.sciencedirect.com/science/article/pii/0550321382905703},
author = {Jonathan R. Finger and Jeffrey E. Mandula},
abstract = {If the effective qq interaction strength due to gluon exchange becomes at least moderately strong beyond some distance g2/4π > 98, the perturbative vacuum becomes unstable to the creation of color singlet qq pairs, which indicates that the true vacuum contains a condensate of such pairs. We analyze the condensation by a coherent state variational procedure modeled after the BCS superconductor, and show that this method is equivalent to a self-consistent Hartree-Fock approximation to the Schwinger-Dyson equation for the quark propagator. The condensate leads to a spontaneous breakdown of chiral symmetry in the manner discussed by Nambu and Jona-Lasinio. The massive pseudoparticles with quark quantum numbers are identified with the “constituent” quarks.}
}


